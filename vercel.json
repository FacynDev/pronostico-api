{
    "version": 2,
    "builds": [
        {
            "src": "./src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/.*",
            "dest": "src/routes/weather.routes.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ]
}